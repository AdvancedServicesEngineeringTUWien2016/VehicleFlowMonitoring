package vfma;

public class AppRunner {

	private static LoadSensorData lsd;
	private static LoadCityMapData lcmd;
	
	public static void main(String[] args) {
	
		lsd = new LoadSensorData();
		lcmd = new LoadCityMapData();
		
		try {
			lcmd.getCityMap();
			lsd.getSensorData();
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		
	}
	
}
